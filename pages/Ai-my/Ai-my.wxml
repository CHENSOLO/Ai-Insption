<!-- 顶部状态栏 -->
<view class="bar-box" style="height: {{navBarHeight}}px;">
  <view class="level" style="margin-top: {{barHeight}}px;">
    <view class="level bar" bindtap="headClick" wx:if="{{show}}">
      <image class="top-head" src="../../images/ai_my/news.png" mode="widthFix"></image>
      <view class="welcome">
        <text class="nick">JefferyTan</text>
        <text class="acount">189****8888</text>
      </view>
    </view>
  </view>
</view>
<!-- 头像模块栏  -->
<view class="head-box" style="padding-top: {{navBarHeight+20}}px;">
  <view class="level" bindtap="getLoginUser">
    <image class="top-head" src="../../images/ai_my/news.png" mode="widthFix"></image>
    <view class="level head-right">
      <view class="welcome">
        <text class="nick" style="font-size: 30rpx;">JefferyTan</text>
        <text class="acount" style="font-size: 28rpx;">189****8888</text>
      </view>
      <!-- <view class="level set-center">已认证</view> -->
    </view>
  </view>
  <view class="level share-box" bindtap="detailClick">
    <view>
      <view class="money">
        <!-- <text>AI助你成为大博</text> -->
        <image class="vip" src="../../images/ai_my/VIP.png"></image>
        <text class="money-text">VIP会员专享特权</text>
      </view>
      <view class="share-text">AI助你开启致富的道路！</view>
    </view>
    <view class="share-btn">联系客服</view>
  </view>
</view>
<!-- 次数加油站模块 -->
<view class="fighting-box">
  <image class="v-bar" src="../../images/ai_my/v_bar.png"></image>
  <text class="v-title">次数加油站</text>
</view>
<view class="white-box">
  <button class="row" style="width: 100%;" bindtap="getShare">
    <view class="left">
      <image class="image-right" src="../../images/ai_my/add.png"></image>
      <text class="text">获取次数</text>
    </view>
    <image class="right" src="../../images/ai_my/right.png" mode="widthFix"></image>
  </button>
</view>
<!-- 其他设置模块 -->
<view class="fighting-box">
  <image class="v-bar" src="../../images/ai_my/v_bar.png"></image>
  <text class="v-title">其他设置</text>
</view>
<view class="white-box">
  <button open-type="share" class="row" style="width: 100%;">
    <view class="left">
      <image class="image-right" src="../../images/ai_my/follow.png"></image>
      <text class="text">关注公众号</text>
    </view>
    <image class="right" src="../../images/ai_my/right.png" mode="widthFix"></image>
  </button>
  <button class="row" style="width: 100%;" bindtap="feedbackClick">
    <view class="left">
      <image class="image-right" src="../../images/ai_my/answer.png"></image>
      <text class="text">使用反馈</text>
    </view>
    <image class="right" src="../../images/ai_my/right.png" mode="widthFix"></image>
  </button>
  <button class="row" style="width: 100%;" bindtap="aboutClick">
    <view class="left">
      <image class="image-right" src="../../images/ai_my/contact.png"></image>
      <text class="text">联系客服</text>
    </view>
    <image class="right" src="../../images/ai_my/right.png" mode="widthFix"></image>
  </button>
  <button open-type="contact" class="row" style="width: 100%;border-bottom: none;">
    <view class="left">
      <image class="image-right" src="../../images/ai_my/about_me.png"></image>
      <text class="text">关于我们</text>
    </view>
    <image class="right" src="../../images/ai_my/right.png" mode="widthFix"></image>
  </button>
</view>
<!-- 底部栏备注 -->
<view class="copy">
  <text>©分享更多示例·请进我主页·点赞加关注·谢谢呦</text>
</view>