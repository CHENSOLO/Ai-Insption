.ai-copywrite-title{
  padding-left: 20rpx;
  padding-right: 15rpx;
  margin-left: 20rpx;
  font-size: 27rpx;
  font-weight: bold;
  margin-bottom: 15rpx;

}
.ai-copywrite-content {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-left: 25rpx;
}

.ai-copywrite-content text {
  border: 1px solid rgb(177, 107, 16);
  border-radius: 14px;
  width: 100rpx;
  text-align: center;
  line-height: 65rpx;
  margin-left: 12rpx;
  margin-bottom: 12rpx;
}
.selected{
  background-color: aqua;
}
